@inject ILogger<ProgramContainerComponent> logger
@inject IProgramContainer programContainer
@inject ISubscriber<ProgramLaunchMessage> programLaunchSubcriber

<div class="position-fixed w-100 h-100 user-select-none overflow-hidden" style="pointer-events:none;">
    @foreach (var renderFramgment in this.programContainer.GetProcesses().Select(process => process.ProgramRenderFramgment))
    {
        @renderFramgment
    }
</div>